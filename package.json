{
  "name": "bears-and-bulls",
  "version": "1.0.0",
  "description": "React Mock Stock Market trading app",
  "main": "server.js",
  "repository": "git@github.com:smalldoorman/bears-and-bulls.git",
  "author": "Nema Darban <nema.darban@gmail.com>",
  "license": "MIT",
  "scripts": {
    "server": "node server.js",
    "client": "node scripts/start-client.js",
    "start": "concurrently \"nodemon server.js\" \"npm run client\"",
    "build": "node scripts/build.js",
    "seed": "node scripts/seedDB.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "installDeps": "yarn && cd client && yarn"
  },
  "jest": {
    "setupFiles": [
      "./tests/jestsetup.js"
    ],
    "testRegex": ".*?(\\.test).js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest/build/index.js"
    },
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "dependencies": {
    "@types/auth0-js": "^8.11.2",
    "auth0-js": "^9.5.0",
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "echarts": "^4.0.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "express": "^4.16.3",
    "jwt-decode": "^2.2.0",
    "moment": "^2.22.1",
    "mongoose": "^5.0.16",
    "morgan": "^1.9.0",
    "path": "^0.12.7",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "jest": "^22.4.3",
    "react-test-renderer": "^16.3.2"
  }
}
